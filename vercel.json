{
    "builds": [
      {
        "src": "nuxt.config.js",
        "use": "@nuxtjs/vercel-builder",
        "config": {}
      }
    ],
    "routes": [
        { "src": "/api/hello(.*)", "dest": "api/hello" },
        { "src": "/api/(.*)", "dest": "api/$1" },
        { "src": "/api", "dest": "api/index.js" }
    ]
}